{
    "apiVersion": "v1",
    "kind": "scenarios",
    "metadata": {
        "name": "scenario-tcp-example-1",
        "labels": {
            "host": "localhost"
        }
    },
    "spec": {
        "active": true,
        "description": "TCP Prob checking if TCP port is open",
        "schedule": "* * * * *",
        "prob": {
            "kind": "tcp",
            "spec": {
                "host": "localhost",
                "port": "8080"
            }
        }
    }
}